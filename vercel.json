{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "tiendas/**/*.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/public/img/(.*)",
      "headers": {
        "Cache-Control": "public, max-age=604800, immutable",
        "CDN-Cache-Control": "public, max-age=604800"
      }
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1",
      "headers": {
        "Cache-Control": "public, max-age=60"
      }
    },
    {
      "src": "/tienda-(.+)/?$",
      "dest": "/tiendas/$1/index.html",
      "headers": {
        "Cache-Control": "public, max-age=3600"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/tiendas/default/index.html"
    }
  ],
  "env": {
    "ENABLE_EDGE": "1"
  }
}
