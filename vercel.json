{
  "version": 2,
  "functions": {
    "api/products.js": {
      "runtime": "edge",
      "includeFiles": "data/products.json"
    },
    "api/whatsapp.js": {
      "runtime": "edge",
      "includeFiles": "data/whatsapp_numbers.json"
    }
  },
  "routes": [
    {
      "src": "/(img|imagenes)/(.*\\.webp)",
      "headers": {
        "Cache-Control": "public, max-age=604800, immutable"
      }
    },
    {
      "src": "/api/products",
      "dest": "/api/products.js"
    },
    {
      "src": "/api/whatsapp",
      "dest": "/api/whatsapp.js"
    },
    {
      "src": "/tienda-(?<store>[^/]+)/?$",
      "dest": "/tiendas/$store/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/tiendas/default/index.html"
    }
  ]
}
